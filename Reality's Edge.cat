              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="7f1b-e57e-e744-407e" type="max"/>
              </constraints>
              <profiles>
                <profile id="0e1d-48f1-59d6-e33f" name="Nerf-A-Lyze" publicationId="35c0-4280-5bd0-3fd1" page="177" hidden="false" typeId="64c0-8b69-4bea-9a66" typeName="Apps">
                  <characteristics>
                    <characteristic name="AP" typeId="7c78-caf5-10a5-3243">1</characteristic>
                    <characteristic name="Range" typeId="206a-a420-1fef-4b05">12</characteristic>
                    <characteristic name="Description" typeId="4b98-82fb-4ecf-4817">Nerf-a-lyze can target any weapon, cyber body part, individual cybernetic enhancement, bot, or Cyborg. It has a +2 CYB bonus and a range of 12”. On a success, the target suffers the Nerf status with a -2 penalty for D3 turns. On a Critical, Nerf-a-lyze disables the target for D3 turns, instead. On a Fumble, the Hacker’s crew gains a Trace token.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name=" Slots" typeId="2582-92bf-6634-1fe3" value="0.0"/>
