              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="ecf0-044a-fdf6-ab69" type="max"/>
              </constraints>
              <profiles>
                <profile id="021f-405d-68dc-a730" name="Slice" publicationId="35c0-4280-5bd0-3fd1" page="178" hidden="false" typeId="64c0-8b69-4bea-9a66" typeName="Apps">
                  <characteristics>
                    <characteristic name="AP" typeId="7c78-caf5-10a5-3243">1</characteristic>
                    <characteristic name="Range" typeId="206a-a420-1fef-4b05">12&quot;</characteristic>
                    <characteristic name="Description" typeId="4b98-82fb-4ecf-4817">Slice targets models with DHP, and provides a +1 CYB bonus. On a success, the target suffers D3+1 digital damage. On a Critical, the digital damage is increased to D3+3. Fumbles have no effect.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name=" Slots" typeId="2582-92bf-6634-1fe3" value="0.0"/>
